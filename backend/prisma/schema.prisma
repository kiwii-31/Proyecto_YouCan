generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"

}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Usuario {
  ID_Usuario  Int      @id @default(autoincrement())
  Nombre      String
  Correo      String   @unique
  Edad        Int
  Contrase√±a  Bytes
}

model Habito {
  ID_Habito   Int      @id @default(autoincrement())
